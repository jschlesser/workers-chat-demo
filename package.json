{
  "name": "edge-chat-demo",
  "description": "An edge chat service that runs on Cloudflare Workers using Durable Objects",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:worker\" \"npm run dev:client\"",
    "dev:worker": "npx wrangler dev --local --assets dist",
    "dev:client": "vite",
    "build": "vite build",
    "build:worker": "npx wrangler deploy --dry-run --outdir dist-worker",
    "preview": "npm run build && npx wrangler dev --local",
    "deploy": "npm run build && npx wrangler deploy"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250830.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "concurrently": "^9.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "wrangler": "^4.33.1"
  }
}
